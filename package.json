{
  "name": "studyreact",
  "version": "0.0.1",
  "description": "study react project",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "watch-js": "babel --presets es2015-node4,react src/ --out-dir lib/ -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js && npm run dev-server && npm run server",
    "build": "npm run clean && babel src -d lib --experimental"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwkfreedom/studyreact.git"
  },
  "author": "jwk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jwkfreedom/studyreact/issues"
  },
  "homepage": "https://github.com/jwkfreedom/studyreact#readme",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015-node4": "^2.0.2",
    "babel-preset-react": "^6.3.13",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "jade": "^1.11.0",
    "nodemon": "^1.8.1",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "material-ui":"0.14.0-rc2"
  }
